<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!-- razzle-only properties and settings -->
  <PropertyGroup>
    <OutputPath>$(BinariesDirectory)\bin\$(BuildArchitecture)\ClrEtw</OutputPath>
    <ArchFlavorSubPath>$(_BuildType)\$(_BuildArch)</ArchFlavorSubPath>
    <DefineConstants>RAZZLE_BUILD;$(DefineConstants)</DefineConstants>
    <UseVsVersion>false</UseVsVersion>
    <BuildProjectReferences>true</BuildProjectReferences>
    <CharacterSet>Unicode</CharacterSet>
  </PropertyGroup>  
</Project>