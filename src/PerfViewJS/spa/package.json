{
  "name": "perfviewjs",
  "productName": "PerfViewJS",
  "version": "0.1.1",
  "private": true,
  "description": "PerfView is a free performance-analysis tool that helps isolate CPU and memory-related performance issues.",
  "author": "https://github.com/microsoft/perfview/graphs/contributors",
  "main": "public/electron.js",
  "build": {
    "appId": "com.microsoft.experiemental.prefviewjs",
    "compression": "maximum",
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  },
  "dependencies": {
    "@fluentui/azure-themes": "^8.1.17",
    "@fluentui/react": "^8.16.0",
    "@monaco-editor/react": "^4.1.3",
    "@types/react": "^16.9.53",
    "@types/react-dom": "17.0.5",
    "@types/react-router-dom": "5.1.7",
    "base64url": "^3.0.1",
    "electron-is-dev": "^2.0.0",
    "i18next": "^20.3.1",
    "i18next-chained-backend": "^3.0.2",
    "i18next-http-backend": "^1.2.6",
    "i18next-localstorage-backend": "^3.1.2",
    "monaco-editor": "^0.23.0",
    "portscanner": "^2.2.0",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.2",
    "react-grid-system": "^7.2.0",
    "react-i18next": "^11.10.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "rimraf": "^3.0.2",
    "socket.io": "^4.1.2",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "brotli": "^1.3.2",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "electron": "^13.1.0",
    "electron-builder": "^22.10.5",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "wait-on": "^5.3.0",
    "zlib": "^1.0.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "start": "rimraf ./build && react-scripts start",
    "prettier:fix": "prettier --write 'src/**/*.ts(x)'",
    "compress": "node compress.js",
    "build": "react-scripts build",
    "test": "cross-env CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "proxy": "http://localhost:5000",
  "browserslist": [
    ">5%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
