    <EmbeddedResource Include="..\PerfView\SupportFiles\Eula.rtf">
      <Type>Non-Resx</Type>
      <WithCulture>false</WithCulture>
      <LogicalName>.\EULA.rtf</LogicalName>
    </EmbeddedResource>

    <EmbeddedResource Include="..\PerfView\SupportFiles\UsersGuide.htm">
